#!/usr/bin/env bash

function install {
    yarn install
}

function build {
    npx plaintext build src -o out
}

function t {
    echo ""
}

function upload {
    build && \
        copy out/* ~/xyheme/inner-universe && \
        pushd ~/xyheme/inner-universe && \
        git add . && \
        git commit -m "up" && \
        git push && \
        popd
}

function main {
    for TASK in "$@"
    do
        "$TASK"
    done
    exit
}

main "$@"
